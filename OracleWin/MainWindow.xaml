<Window x:Class="Zyrenth.OracleHack.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xceed="clr-namespace:Xceed.Wpf.Toolkit;assembly=Xceed.Wpf.Toolkit"
        Title="Oracle of Secrets" Height="305" Width="306" Icon="/OracleWin;component/Farore.ico">
    <Grid>
        <GroupBox Header="Secret For..." Height="79" HorizontalAlignment="Left" Margin="27,12,0,0" Name="groupBox1" VerticalAlignment="Top" Width="111">
            <Grid>
                <Image Height="20" HorizontalAlignment="Left" Margin="6,6,0,0" Name="image1" Stretch="Fill" VerticalAlignment="Top" Width="20" Source="/OracleWin;component/Images/Nayru.gif" />
                <Image Height="20" HorizontalAlignment="Left" Margin="6,32,0,0" Name="image2" Source="/OracleWin;component/Images/Din.gif" Stretch="Fill" VerticalAlignment="Top" Width="20" />
                <RadioButton Content="Ages" Height="16" HorizontalAlignment="Left" Margin="32,8,0,0" Name="radioButton1" VerticalAlignment="Top" />
                <RadioButton Content="Seasons" Height="16" HorizontalAlignment="Left" Margin="32,34,0,0" Name="radioButton2" VerticalAlignment="Top" />
            </Grid>
        </GroupBox>
        <GroupBox Header="Game Type" Height="79" HorizontalAlignment="Left" Margin="144,12,0,0" Name="groupBox2" VerticalAlignment="Top" Width="111">
            <Grid>
                <RadioButton Content="Linked Quest" Height="16" HorizontalAlignment="Left" Margin="6,8,0,0" Name="radioButton3" VerticalAlignment="Top" />
                <RadioButton Content="Hero's Quest" Height="16" HorizontalAlignment="Left" Margin="6,34,0,0" Name="radioButton4" VerticalAlignment="Top" />
            </Grid>
        </GroupBox>
        <TextBox Height="23" HorizontalAlignment="Left" Margin="56,111,0,0" Name="textBox1" VerticalAlignment="Top" Width="68" MaxLength="5" />
        <xceed:ShortUpDown Height="23" HorizontalAlignment="Left" Margin="56,140,0,0" Name="textBox2" VerticalAlignment="Top" Width="68" Minimum="0" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="192,111,0,0" Name="textBox3" VerticalAlignment="Top" Width="68" MaxLength="5" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="56,169,0,0" Name="comboBox1" VerticalAlignment="Top" Width="68" />
        <ComboBox Height="23" HorizontalAlignment="Left" Margin="192,140,0,0" Name="comboBox2" VerticalAlignment="Top" Width="68" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="16,114,0,0" Name="textBlock1" Text="Hero" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="16,140,0,0" Name="textBlock2" Text="ID" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="16,169,0,0" Name="textBlock3" Text="Friend" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="140,114,0,0" Name="textBlock4" Text="Child" VerticalAlignment="Top" />
        <TextBlock Height="23" HorizontalAlignment="Left" Margin="140,140,0,0" Name="textBlock5" Text="Behavior" VerticalAlignment="Top" />
    </Grid>
</Window>
